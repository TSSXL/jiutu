  flex-direction: row;
  background-color: white;
}
.tab text{
  width:20%;
  height:100%;
  text-align: center;
  line-height: 40px;
  color:#808080;
  font-size: 1.15em;
  letter-spacing: 1px;
}
.item{
  display: flex;
  flex-direction: column;
  width:100%;
  margin-top: 15rpx;
}
.itemOne{
  width:97.5%;
  height:80rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
