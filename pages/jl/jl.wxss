.jlCon{
  width:100%;
  background-color: #f7f7f7;
  font-size: 62.5%;
  padding-bottom: 40rpx;
}
.item{
  display: flex;
  flex-direction: row;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  background-color: white;
  border-bottom: 1px solid #cccccc;
  width:97.5%;
  padding-left: 2.5%;
}
.item text{
  display: block;
  height:80rpx;
  font-size: 1.3em;
  font-weight: bold;
color:#333333;
line-height: 40px;
letter-spacing: 1px;
width:60%;
}
.picker{
  height:80rpx;
color:#808080;
line-height: 40px;
letter-spacing: 3rpx;
position: absolute;
right:6%;
}
.item image{
  width:40rpx;
  height:40rpx;
  position: absolute;
  right:1%;
  margin-top: 19rpx;
}
.item input{
    height:80rpx;
color:#808080;
line-height: 40px;
letter-spacing: 3rpx;
position: absolute;
right:1%;
width:28%;
}
.xz{
  position: absolute;
  right:6%;
  font-size: 1.1em !important;
  width:22% !important;
  text-align: right;
  color:#4D4D4D !important;
}
.e{
  width:100%;
  background-color: white;
  padding-top: 10rpx;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #cccccc;
}
textarea{
  width:95%;
  margin-left: 2.5%;
  border-radius: 5rpx;
  margin-top: 20rpx;
background-color: #f7f7f7;
line-height: 25px;
color:#808080;
font-weight: bold;
letter-spacing: 1px;
height:180px;
}
.upload{
  width:100%;
  display: flex;
  flex-direction: row;
  background-color: white;
  padding-top: 10rpx;
